{
  "name": "onpuppet-monitoring",
  "version": "2.2.2",
  "author": "Jon Skarpeteig",
  "summary": "Autodetect and install monitoring and metrics for supported services",
  "license": "Apache-2.0",
  "source": "git@github.com:onpuppet/puppet-monitoring.git",
  "project_page": "https://github.com/onpuppet/puppet-monitoring",
  "issues_url": "https://github.com/onpuppet/puppet-monitoring",
  "data_provider": "hiera",
  "tags": [
    "fact",
    "facter",
    "facts",
    "monitor",
    "monitoring",
    "collectd"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "OracleLinux",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Scientific",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04",
        "16.04"
      ]
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 3.2.0 < 5.0.0"
    },
    {
    	"name": "puppet-collectd",
    	"version_requirement": ">= 5.0.0 < 9.9.9"
    },
    {
      "name": "puppetlabs-apt",
      "version_requirement": ">= 2.2.0 < 5.0.0"
    },
    {
    	"name": "sensu/sensu",
    	"version_requirement": ">= 2.1.0 < 5.0.0"
    },
    {
    	"name": "onpuppet-refacter",
    	"version_requirement": ">= 1.0.0 < 9.9.9"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.5.0 < 6.0.0"
    }
  ]
}

